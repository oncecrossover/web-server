<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.gibbon.peeq.db.model.Journal" dynamic-update="true"
  dynamic-insert="true" table="Journal">
    <id name="id" type="long">
      <column name="id" />
      <generator class="increment" />
    </id>
    <property name="transactionId" type="long">
      <column name="transactionId" />
    </property>
    <property name="uid" type="java.lang.String">
      <column name="uid" />
    </property>
    <property name="amount" type="java.lang.Double">
      <column name="amount" />
    </property>
    <property name="type" type="java.lang.String">
      <column name="type" />
    </property>
    <property name="chargeId" type="java.lang.String">
      <column name="chargeId" />
    </property>
    <property name="status" type="java.lang.String">
      <column name="status" />
    </property>
    <property name="originId" type="long">
      <column name="originId" />
    </property>
    <property name="createdTime" type="timestamp">
      <column name="createdTime" />
    </property>
  </class>
</hibernate-mapping>