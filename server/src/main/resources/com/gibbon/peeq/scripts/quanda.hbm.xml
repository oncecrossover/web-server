<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.gibbon.peeq.db.model.Quanda" dynamic-update="true"
  table="Quanda">
    <id name="id" type="long">
      <column name="id" />
      <generator class="increment" />
    </id>
    <property name="asker" type="java.lang.String">
      <column name="asker" />
    </property>
    <property name="question" type="java.lang.String">
      <column name="question" />
    </property>
    <property name="responder" type="java.lang.String">
      <column name="responder" />
    </property>
    <property name="rate" type="java.lang.Double">
      <column name="rate" />
    </property>
    <property name="answerUrl" type="java.lang.String">
      <column name="answerUrl" />
    </property>
    <property name="status" type="java.lang.String">
      <column name="status" />
    </property>
    <property name="active" type="java.lang.String">
      <column name="active" />
    </property>
    <property name="createdTime" type="timestamp">
      <column name="createdTime" />
    </property>
    <property name="updatedTime" type="timestamp">
      <column name="updatedTime" />
    </property>
  </class>
</hibernate-mapping>
