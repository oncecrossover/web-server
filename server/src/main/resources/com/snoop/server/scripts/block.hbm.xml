<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.snoop.server.db.model.BlockEntry" dynamic-update="true"
  dynamic-insert="true" table="Block">
    <id name="id" type="long">
      <column name="id" />
      <generator class="com.snoop.server.id.UniqueIdGenerator" />
    </id>
    <property name="uid" type="long">
      <column name="uid" />
    </property>
    <property name="blockeeId" type="long">
      <column name="blockeeId" />
    </property>
    <property name="blocked" type="java.lang.String">
      <column name="blocked" />
    </property>
    <property name="createdTime" type="timestamp">
      <column name="createdTime" />
    </property>
    <property name="updatedTime" type="timestamp">
      <column name="updatedTime" />
    </property>
  </class>
</hibernate-mapping>