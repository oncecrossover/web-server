<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.snoop.server.db.model.PcAccount" dynamic-update="true"
  dynamic-insert="true" table="PcAccount">
    <id name="id" type="long">
      <column name="id" />
      <generator class="foreign">
        <param name="property">user</param>
      </generator>
    </id>
    <property name="chargeFrom" type="java.lang.String">
      <column name="chargeFrom" />
    </property>
    <property name="payTo" type="java.lang.String">
      <column name="payTo" />
    </property>
    <property name="createdTime" type="timestamp">
      <column name="createdTime" />
    </property>
    <property name="updatedTime" type="timestamp">
      <column name="updatedTime" />
    </property>
    <one-to-one name="user" class="com.snoop.server.db.model.User"
      constrained="true" />
  </class>
</hibernate-mapping>
